package com.manthan.object.test;

public class Student {
	String name;
	int id;
	double per;
	public Student(String name, int id, double per) {
		this.name = name;
		this.id = id;
		this.per = per;
	}
	@Override
	public String toString() {
		return "Name: "+name+"ID: "+id+"Percentage: "+per;
	}
	
	
	
		
	

}
